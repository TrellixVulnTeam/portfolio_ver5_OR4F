    <script>
        let getNickname = window.localStorage.getItem('nickname');

        var greeting = `ÏïàÎÖïÌïòÏÑ∏Ïöî ${getNickname}Îãò. Î∞òÎÇòÏÑú Î∞òÍ∞ëÏäµÎãàÎã§!`
        $('.greeting').html(greeting)

        // Ïä§ÌÅ¨Î°§
        var mHtml = $("html");
        var page = 1;
        $(window).on("wheel", function (e) {
            if (mHtml.is(":animated")) 
                return;
            if (e.originalEvent.deltaY > 0) {
                if (page == 6) 
                    return;
                page++;
            } else if (e.originalEvent.deltaY < 0) {
                if (page == 1) 
                    return;
                page--;
            }
            var posTop = (page - 1) * $(window).height();
            mHtml.animate({scrollTop: posTop});
        })
    </script>
